{
    consul: "{{ .CONSUL_ADDRESS | default "localhost" }}:{{ .CONSUL_PORT | default "8500" }}",
    jobs: [
        {
            name: "scheduler",
            port: 8080,
            exec: "java -Dhudson.model.DirectoryBrowserSupport.CSP= -Dmail.smtp.starttls.enable=true -jar /app/jenkins.war",
            restarts: "unlimited",
            health: {
                exec: 'curl -sL localhost:8080 -o /dev/null --max-time 15',
                interval: 20,
                ttl: 60,
                timeout: "40s"
            },
            when: {
                source: "watch.consul",
                once: "healthy"
            }
        }
    ],
    watches: [
        {
            name: "consul",
            interval: 5
        }
    ]
}